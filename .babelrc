{
    "presets": [
        "next/babel"
    ],
    "plugins": [
		"@babel/plugin-proposal-optional-chaining",
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "src": "./src"
                }
            }
        ],
        [
            "import",
            {
				"libraryName": "antd",
				"libraryDirectory": "lib",
                "style": true
            }
        ]
	],
	"ignore": ["node_modules"]
}
