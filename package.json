{
  "name": "boilerplate-nextjs-antd-less",
  "version": "2.0.0",
  "main": "index.js",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-saga, antd, less",
  "repository": {
    "type": "git",
    "url": "git@github.com:tientran0019/boilerplate-nextjs-antd-less.git"
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "redux-saga",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "next",
    "build": "yarn clean && NODE_ENV=production next build",
    "start": "next start",
    "export": "next export",
    "build:docker-compose": "docker-compose build",
    "start:docker-compose": "docker-compose up -d",
    "build:docker": "docker build -t web-image .",
    "start:docker": "docker run --name web-container -p 3000:3000 -d web-image"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.2.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.1.0",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.1.0",
    "next-optimized-images": "^2.6.0",
    "nprogress": "^0.2.0",
    "null-loader": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.13.1",
    "react-use": "^14.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "frontmatter-markdown-loader": "^3.4.2",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
