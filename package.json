{
  "name": "boilerplate-nextjs-antd-less",
  "version": "2.0.0",
  "main": "index.js",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-saga, antd, less",
  "repository": {
    "type": "git",
    "url": "git@github.com:tientran0019/boilerplate-nextjs-antd-less.git"
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "redux-saga",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "next",
    "build": "yarn clean && NODE_ENV=production next build",
    "start": "next start",
    "export": "next export",
    "build:docker-compose": "docker-compose build",
    "start:docker-compose": "docker-compose up -d",
    "build:docker": "docker build -t web-image .",
    "start:docker": "docker run --name web-container -p 3000:3000 -d web-image"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@babel/core": "^7.12.10",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.9.4",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "less": "^3.13.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "next": "^10.0.4",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "nprogress": "^0.2.0",
    "null-loader": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.1",
    "react-use": "^15.3.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
